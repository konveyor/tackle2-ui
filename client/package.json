{
  "name": "@konveyor-ui/client",
  "version": "0.1.0",
  "type": "commonjs",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "clean": "rimraf ./dist ./coverage",
    "prebuild": "npm run clean && npm run tsc -- --noEmit",
    "build": "NODE_ENV=production webpack --config ./config/webpack.prod.ts",
    "build:dev": "NODE_ENV=development webpack --config ./config/webpack.dev.ts",
    "start:dev": "NODE_ENV=development webpack serve --config ./config/webpack.dev.ts",
    "test": "NODE_ENV=test TZ=UTC jest --rootDir=. --config=./config/jest.config.ts",
    "format": "prettier --ignore-path ../.gitignore --ignore-path ../.prettierignore --ignore-unknown --write .",
    "format:check": "prettier --ignore-path ../.gitignore --ignore-path ../.prettierignore --ignore-unknown --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsc": "tsc -p ./tsconfig.json"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^2.9.11",
    "@microlink/react-json-view": "^1.27.0",
    "@monaco-editor/react": "^4.7.0",
    "@patternfly/patternfly": "5.4.2",
    "@patternfly/react-charts": "7.4.9",
    "@patternfly/react-code-editor": "5.4.18",
    "@patternfly/react-core": "5.4.14",
    "@patternfly/react-table": "5.4.16",
    "@patternfly/react-tokens": "5.4.1",
    "@react-keycloak/web": "3.4.0",
    "@tanstack/react-query": "^4.40.1",
    "@tanstack/react-query-devtools": "^4.40.1",
    "axios": "^1.9.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.13",
    "fast-xml-parser": "^5.2.5",
    "file-saver": "^2.0.5",
    "git-url-parse": "^16.1.0",
    "i18next": "^25.3.0",
    "i18next-http-backend": "^3.0.2",
    "immer": "^10.1.3",
    "js-yaml": "^4.1.0",
    "keycloak-js": "^26.1.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.52.2",
    "radash": "^12.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.58.1",
    "react-i18next": "^15.5.2",
